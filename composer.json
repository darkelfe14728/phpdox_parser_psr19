    "description":       "A phpDox parser for PSR-19 documentation tags",
    "keywords":          [ "phpDox", "PSR-19", "parser" ],

    "minimum-stability": "stable",
    "require":           {
        "php": ">=7.2",
        "theseer/phpdox": "^0.12.0"
    },
    "autoload":          {
